.open-vip-list {
  padding: 30rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-flex-wrap: wrap;
          flex-wrap: wrap;
}
.open-vip-list .vip-list-item {
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
  margin-bottom: 20rpx;
  width: 32%;
  border: 1rpx solid #ccc;
  -webkit-border-radius: 10rpx;
          border-radius: 10rpx;
  margin-right: 2%;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
          flex-direction: column;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  padding: 20rpx;
  background-color: #F7F7F7;
}
.open-vip-list .vip-list-item:nth-child(3n) {
  margin-right: 0;
}
.open-vip-list .vip-list-item.selete {
  background-color: #FFE4E9;
  border-color: #DF5F8E;
}
.open-vip-list .vip-list-item .vip-list-item-pic {
  margin: 20rpx 0;
  color: #D3155B;
}
.open-vip-list .vip-list-item .vip-list-item-pic .vip-list-item-num {
  font-size: 60rpx;
  font-weight: bold;
  margin-right: 5rpx;
}
.open-vip-list .vip-list-item .vip-list-item-bottom {
  color: #474747;
}

