<view><block wx:if="{{cartList.length!==0}}"><view class="integral-cart-list"><block wx:for="{{cartList}}" wx:for-item="item" wx:for-index="__i0__"><view class="integral-cart-item flex align-center"><view><label><block wx:if="{{item.is_checked===1}}"><checkbox data-id="{{item.id}}" data-checkId="{{item.is_checked}}" data-event-opts="{{[['tap',[['choice',['$event']]]]]}}" bindtap="__e"></checkbox></block><block wx:if="{{item.is_checked===2}}"><checkbox checked="{{true}}" data-id="{{item.id}}" data-checkId="{{item.is_checked}}" data-event-opts="{{[['tap',[['choice',['$event']]]]]}}" bindtap="__e"></checkbox></block></label></view><view class="integral-cart-item-center flex"><image class="mx-3" src="{{hostUrl+item.litpic}}"></image><view class="flex flex-column font-m"><text>{{item.title}}</text><text class="text-muted my-1">{{"x"+item.quantity}}</text></view></view><view class="integral-cart-item-bottom flex flex-column"><text class="font-lg text-pic">{{item.integral}}</text><image class="delete-img" src="../../static/images/cartImg/delete.png" data-event-opts="{{[['tap',[['delItem',['$0'],[[['cartList','id+item.title',item.id+item.title,'id']]]]]]]}}" bindtap="__e"></image></view></view></block></view></block><block wx:else><view class="text-center mt-5 font-md text-secondary">购物车空空如也~</view></block></view>