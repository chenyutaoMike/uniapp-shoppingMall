<view><block wx:if="{{shopList.length!==0}}"><view class="shop-list-box flex flex-wrap justify-between"><block wx:for="{{shopList}}" wx:for-item="item" wx:for-index="index"><block><view class="shop-list-item"><view class="shop-list-img"><image src="{{hostUrl+item.litpic}}"></image></view><view class="item-content"><text class="item-title font-md">{{item.name}}</text><view class="item-bottom flex justify-between align-center"><view class="flex flex-column"><text class="item-new-pic">{{"积分:"+item.integral}}</text></view><view data-event-opts="{{[['tap',[['addCart',['$0'],[[['shopList','index+item.name',index+item.name,'id']]]]]]]}}" class="item-shop-cart" catchtap="__e"><image src="{{shopCartImg}}"></image></view></view></view></view></block></block></view></block><block wx:else><view class="text-center my-4 font-lg text-secondary">暂无商品</view></block></view>