<view class="classification"><view class="top"><input class="classification-input" type="text" confirm-type="search" placeholder="搜索商品名称" data-event-opts="{{[['confirm',[['submitValue',['$event']]]],['input',[['__set_model',['','classificationSearch','$event',[]]]]]]}}" value="{{classificationSearch}}" bindconfirm="__e" bindinput="__e"/></view><view class="classificationpage"><view class="tui-fixed-left"><scroll-view class="tui-city-scroll" style="height:92%;" scroll-x="true" scroll-y="true" scroll-with-animation="true" data-event-opts="{{[['scroll-top',[['leftMenuTop',['$event']]]]]}}" bindscroll-top="__e"><block wx:for="{{typeList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="{{[index===currentActiveIndex?'menu-item menu-active ':'menu-item']}}" data-index="{{index}}" id="{{index}}" data-event-opts="{{[['tap',[['changeMenu',['$event']]]]]}}" bindtap="__e">{{item.title}}</view></block></scroll-view></view><view class="content"><scroll-view class="tui-city-scroll-y" style="height:92%;" scroll-y="true" scroll-with-animation="true" scroll-top="{{rightProTop}}" data-event-opts="{{[['scroll',[['scroll',['$event']]]]]}}" bindscroll="__e"><block wx:for="{{typeList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="pro-item"><view class="tui-list-head" id="{{'NAV'+index}}">{{item.title}}</view><view class="tui-list-li"><block wx:for="{{item.list}}" wx:for-item="itemList" wx:for-index="index" wx:key="index"><view class="goods-box" data-id="{{itemList.id}}" data-event-opts="{{[['tap',[['goGoodList',['$event']]]]]}}" bindtap="__e"><image class="goodsImg" src="{{hostUrl+itemList.img_url}}"></image><view class="goodsName">{{itemList.title}}</view></view></block></view></view></block></scroll-view></view></view></view>